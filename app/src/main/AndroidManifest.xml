<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.spotcheck">
    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/y"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Spotcheck"
        tools:targetApi="31">

        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginAct"/>
        <activity android:name=".DaftarAct"/>
        <activity android:name=".User_Dashboard"/>
        <activity android:name=".User_PertanyaanCek_Act"/>
        <activity android:name=".User_Hasil_Act"/>
        <activity android:name=".Admin_Dashboard"/>
        <activity android:name=".Admin_Pertanyaan"/>
        <activity android:name=".Admin_TambahPertanyaan"/>
        <activity android:name=".Admin_EditPertanyaan"/>
        <activity android:name=".Admin_Penyakit"/>
        <activity android:name=".Admin_TambahPenyakit"/>



        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />


    </application>

</manifest>